{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@cv-es": [
        "./cv-es.json"
      ],
      "@cv-en": [
        "./cv-en.json"
      ],
      "@/sections/*": [
        "./src/components/sections/*"
      ],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}